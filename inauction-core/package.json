{
  "name": "inauction-core",
  "version": "1.0.0",
  "description": "inauction-core backend infrastructure source code",
  "main": "index.js",
  "scripts": {
    "test": "ts-node ./lib/server.ts",
    "serve": "nodemon ./lib/server.ts",
    "dev": "tsc && nodemon ./dist/server.js",
    "prod": "tsc && NODE_ENV=prod nodemon -r dotenv/config ./dist/server.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "build": "tsc -p .",
    "pm2:prod": "NODE_ENV=prod pm2 start ./dist/server.js --watch -i max -f --name \"api-core\""
  },
