    "@types/jest": "^26.0.15",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.11.2",
    "@wufe/mapper": "^0.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "guid-typescript": "^1.0.9",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
